#include "holberton.h"
#include <stdio.h>

/**
 * print_array - prints elements of an array of ints.
 * @a: an element within the array.
 * @n: number of elements within the array.
 *
 */
void print_array(int *a, int n);
{
	int num;

	for (num = 0 ; a[num] < n ; num++)
	{
		if (num == (n - 1))
			printf("%d", a[num]);
		else
			printf("%d, ", a[num]);
	}
	_putchar('\n');
}
