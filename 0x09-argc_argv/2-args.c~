#include <stdio.h>

/**
 * main - prints all arguments received.
 * @argc: number of arguments passed to the program.
 * @argv: arguments passed to the program
 * @i: index for loop
 *
 * Return: 0
 */

int main(int argc, char *argv[])
{
	int i;

	for(i = 0 ; i < argc; i++)
	{
		printf("argv[%d] = %s\n", i, argv[i]);
	}
	return (0);
}
